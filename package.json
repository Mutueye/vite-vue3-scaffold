{
  "name": "vite-vue3-scaffold",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "packageManager": "pnpm@7.18.1",
  "homepage": "https://mutyeye.github.io/vite-vue3-scaffold/",
  "scripts": {
    "prepare": "simple-git-hooks",
    "dev": "vite --mode development",
    "dev:tev": "vite --mode test",
    "dev:prod": "vite --mode production",
    "dev:local": "vite --mode local",
    "dev:release": "vite --mode release",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .eslintignore --fix src/",
    "lint:check": "eslint --ext .js,.ts,.vue --ignore-path .eslintignore src/",
    "lint:style": "stylelint ./src/**/*.{vue,css,scss} --fix",
    "lint:style:check": "stylelint ./src/**/*.{vue,css,scss}",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:update": "vitest --update",
    "test:watch": "vitest",
    "deploy:pages": "gh-pages -d dist"
  },
  "dependencies": {
    "@iconify/json": "^2.2.3",
    "@itshixun/qst-ui-system": "^0.0.2",
    "@vueuse/core": "^9.10.0",
    "@vueuse/head": "^1.0.22",
    "axios": "^1.2.2",
    "color2k": "^2.0.1",
    "element-plus": "^2.2.28",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.0",
    "pinia": "^2.0.28",
    "pinia-plugin-persist": "^1.0.0",
    "vanilla-colorful": "^0.7.2",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.26.3",
    "@vue/test-utils": "^2.2.7",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-vue": "^9.8.0",
    "gh-pages": "^4.0.0",
    "jsdom": "^20.0.3",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.20",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.1",
    "sass": "^1.57.1",
    "simple-git-hooks": "^2.8.1",
    "stylelint": "^14.16.1",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recess-order": "^3.1.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-scss": "^4.3.0",
    "typescript": "^4.9.4",
    "unocss": "^0.48.0",
    "vite": "^4.0.4",
    "vitest": "^0.26.3",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.0.22"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,json}": "eslint --ignore-path .eslintignore --fix",
    "*.{vue,js,ts,jsx,tsx,md,json,scss,css,html}": "prettier . --write",
    "*.{scss,css,vue}": "stylelint --fix"
  }
}
