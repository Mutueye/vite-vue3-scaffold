{
  "name": "vite-vue3-scaffold",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@7.18.1",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
