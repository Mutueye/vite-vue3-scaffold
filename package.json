    "@typescript-eslint/parser": "^5.45.1",
    "@vitejs/plugin-vue": "^3.2.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.8.0",
    "typescript": "^4.9.3",
    "vite": "^3.2.5",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.0.11"
  }
