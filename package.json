{
  "name": "vite-vue3-scaffold",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "packageManager": "pnpm@7.18.1",
  "scripts": {
    "prepare": "simple-git-hooks",
    "dev": "vite --mode development",
    "dev:tev": "vite --mode test",
    "dev:local": "vite --mode local",
    "dev:prod": "vite --mode production",
    "dev:release": "vite --mode release",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore --fix src/",
    "lint:check": "eslint --ext .js,.ts,.vue --ignore-path .gitignore src/",
    "lint:style": "stylelint ./src/**/*.{vue,css,scss} --fix",
    "lint:style:check": "stylelint ./src/**/*.{vue,css,scss}",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:update": "vitest --update",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@iconify/json": "^2.1.152",
    "@vueuse/core": "^9.6.0",
    "@vueuse/head": "^1.0.22",
    "axios": "^1.2.1",
    "element-plus": "^2.2.26",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.28",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@types/node": "^18.11.15",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitest/coverage-c8": "^0.25.8",
    "@vue/test-utils": "^2.2.6",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.8.0",
    "jsdom": "^20.0.3",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.20",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.1",
    "sass": "^1.56.2",
    "simple-git-hooks": "^2.8.1",
    "stylelint": "^14.16.0",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-scss": "^4.3.0",
    "typescript": "^4.9.4",
    "unocss": "^0.47.6",
    "vite": "^3.2.5",
    "vitest": "^0.25.8",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.0.13"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,json}": "eslint --ignore-path .gitignore --fix",
    "*.{vue,js,ts,jsx,tsx,md,json,scss,css,html}": "prettier . --write",
    "*.{scss,css,vue}": "stylelint --fix"
  }
}
